<div class="container">
    <div class="panel panel-default">
        <div class="panel-heading">Best GAMERS</div>
        <table class="table">
            <thead>
                <tr>
                    <th>#</th>
                    <th>Username</th>
                    <th>Scope</th>
                </tr>
            </thead>
            <tbody>
                <% for(var i=0; i<users.length; i++) { %>
                <tr>
                    <th scope="row"><%= place[i] %></th>
                    <th><%= users[i] %></th>
                    <th><%= score[i] %></th>
                </tr>
                <% } %>
                <tr>
                    <th scope="row"><%= userplace %></th>
                    <th>YOU</th>
                    <th><%= userscore %></th>
                </tr>
            </tbody>
        </table>
    </div>
</div>
